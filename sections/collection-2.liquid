{{ 'collection-2.css' | asset_url | stylesheet_tag }}
{% render 'section-style' %}

{% style %}
  #shopify-section-{{ section.id }} {
    --heading-color: {{ section.settings.heading_color | color_extract: 'red' }}, {{ section.settings.heading_color | color_extract: 'green' }}, {{ section.settings.heading_color | color_extract: 'blue' }};
    --color-body: {{ section.settings.text_color | color_extract: 'red' }}, {{ section.settings.text_color | color_extract: 'green' }}, {{ section.settings.text_color | color_extract: 'blue' }};
    --ap-nextap-buttonprev-background: var(--color-body);
    --ap-nextap-buttonprev-color: var(--section-background);
  }
  {% for block in section.blocks %}
    #block-{{ block.id }}-{{ forloop.index }} {
      --section-block-overlay: {{ block.settings.overlay | color_extract: 'red' }}, {{ block.settings.overlay | color_extract: 'green' }}, {{ block.settings.overlay | color_extract: 'blue' }};
      --section-block-overlay-opacity: {{ block.settings.overlay_opacity | divided_by: 10.0 }};
    }
  {% endfor %}

  #shopify-section-{{ section.id }} .list-collections__item-info {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 1;
    padding: {{ section.settings.padding_inner }};
    transform: none;
  }

  #shopify-section-{{ section.id }} .heading.count {
    font-size: 14px;
    line-height: 20px;
    font-weight: 500;
    padding: 4px 16px;
    background-color: var(--primary);
    display: inline-block;
    border-radius: 8px;
    margin: 0 0 10px;
  }

  #shopify-section-{{ section.id }} h2.heading.h5 {
    transition: all .4s;
  }

  #shopify-section-{{ section.id }} .list-collections__item:hover h2.heading.h5 {
    border-bottom: 1px solid rgb(var(--heading-color));
    padding-bottom: 6px;
  }

  #shopify-section-{{ section.id }} .list-collections__item-image-wrapper {
    height: 100%;
  }

  @media (max-width: 767px) {
    #shopify-section-{{ section.id }} .list-collections__item-info {
      padding: 20px;
    }
  }

{% endstyle %}

<section class="section section--flush collection-2">   
  <ap-collectionlist reveal-on-scroll="" class="list-collections list-collections--collage ap-object-loaded">
    <div class="{{ section.settings.setwidthout }}">

      {% render 'section-heading' %}

      <div class="collection-2-content row">

        {%-liquid
          assign position_text = section.settings.position_text

          case position_text
            when 'top-left'
            assign positiontxt = 'align-items-start justify-content-start'
            when 'top-center'
            assign positiontxt = 'align-items-center justify-content-start'
            when 'top-right'
            assign positiontxt = 'align-items-end justify-content-start'
            when 'mid-left'
            assign positiontxt = 'align-items-start justify-content-center'
            when 'mid-center'
            assign positiontxt = 'align-items-center justify-content-center'
            when 'mid-right'
            assign positiontxt = 'align-items-end justify-content-center'
            when 'bot-left'
            assign positiontxt = 'align-items-start justify-content-end'
            when 'bot-center'
            assign positiontxt = 'align-items-center justify-content-end'
            when 'bot-right'
            assign positiontxt = 'align-items-end justify-content-end'
            else 
            assign positiontxt = 'align-items-end justify-content-start'
          endcase
        -%}

        {% for block in section.blocks %}
          {%- case block.settings.width -%}
            {%- when '25%' -%}
            {%- assign block_width = 'col-lg-3 col-md-6 col-12' -%}
            {%- when '28%' -%}
            {%- assign block_width = 'col-lg-28 col-md-6 col-12 mb-30' -%}
            {%- when '33%' -%}
            {%- assign block_width = 'col-xl-4 col-lg-4 col-12' -%}
            {%- when '44%' -%}
            {%- assign block_width = 'col-lg-44 col-md-6 col-12 mb-30' -%}
            {%- when '50%' -%}
            {%- assign block_width = 'col-lg-6 col-md-12 col-12' -%}
            {%- when '66%' -%}
            {%- assign block_width = 'col-xl-8 col-lg-66 col-12' -%}
            {%- when '100%' -%}
            {%- assign block_width = 'col-12' -%}
            {%- endcase -%}
            <div class="{{ block_width }} {{ block.settings.class }}">
              {% case block.type %}

              {%- when 'collection' -%}
              {% liquid
                assign _link = block.settings.collection.url
              %}
              <div class="collection-1-row h-100">
                <a id="block-{{ block.id }}-{{ forloop.index }}" href="{{ _link }}" class="list-collections__item has-overlay image-zoom">
                  <div class="list-collections__item-image-wrapper" style="padding-top:{{ 1 | divided_by: block.settings.featured_image.aspect_ratio | times: 100 }}%;">
                    {% if block.settings.featured_image != blank %}
                      <img class="list-collections__item-image" reveal="" loading="lazy" 
                        sizes="(max-width: 740px) 80vw, (max-width: 999px) 60vw, 425px"
                        alt="" 
                        src="{%- if block.settings.featured_image.width >= 3000 -%}{{ block.settings.featured_image | image_url: width: 3000, crop: 'center' }} 3000w,{%- endif -%}
                          {{ block.settings.featured_image | image_url }} {{ block.settings.featured_image.width }}w"
                        src="{{ block.settings.featured_image | image_url: width: 1500 }}"
                        height="{{ block.settings.featured_image.height }}"
                        width="{{ block.settings.featured_image.width }}">
                    {% else %}
                      <div class="list-collections__item-image">
                        {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                      </div>
                    {% endif %}
                  </div>
                  <div class="list-collections__item-info d-flex flex-column {{ positiontxt }}">

                    <div class="text-{{ section.settings.text_align_item }}">
                      {% if section.settings.show_count %}
                        <span class="heading count" reveal="">{{ block.settings.collection.products_count | append: ' ' | append: ' products' }}</span>
                      {% endif %}
                      <h2 class="heading h{{ section.settings.heading_size_range }}" reveal="">
                        {% if block.settings.heading != blank %}
                          <span>
                            {{- block.settings.heading -}}
                          </span>
                        </h2>
                      {% else %}
                        {{- block.settings.collection.title | escape -}}</p>
                      {% endif %}
                    </div>

                  </div>
                </a>
              </div>

              {%- when 'collection-2-row' -%}
                {% liquid
                assign _link = block.settings.collection.url
                assign _link2 = block.settings.collection2.url
              %}
              <div class="collecton-2-row">
                <div class="collection-top">
                  <a id="block-{{ block.id }}-{{ forloop.index }}" href="{{ _link }}" class="list-collections__item has-overlay image-zoom mb-30">
