                              </svg>
                              <span class="text-social">{{ 'general.social.links.facebook' | t }}</span>
                              </a>
                            </li>
                          {%- endif -%}
                          {%- if block.settings.social_tw != blank -%}
                            <li class="social-media__item social-media__item--tw">
                              <a href="{{ block.settings.social_tw }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on tw">
