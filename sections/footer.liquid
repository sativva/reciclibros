{{ 'footer.css' | asset_url | stylesheet_tag }}

<style>
  #shopify-section-{{ section.id }} {
    {%- if section.settings.background_image != blank -%}
      background-image: url({{ section.settings.background_image | image_url: width: 2000 }});
      background-repeat: repeat;
    {%- endif -%}

    position: relative;
  }
  #shopify-section-{{ section.id }} .footer {
    --background: {{ section.settings.background | color_extract: 'red' }}, {{ section.settings.background | color_extract: 'green' }}, {{ section.settings.background | color_extract: 'blue' }};
    --heading-color: {{ section.settings.heading_color | color_extract: 'red' }}, {{ section.settings.heading_color | color_extract: 'green' }}, {{ section.settings.heading_color | color_extract: 'blue' }};
    --color-body: {{ section.settings.text_color | color_extract: 'red' }}, {{ section.settings.text_color | color_extract: 'green' }}, {{ section.settings.text_color | color_extract: 'blue' }};
    --border-color: {{ section.settings.color_border }};
    --text-link: {{ section.settings.text_link }};
    --section-overlay-opacity: {{ section.settings.overlay_opacity | divided_by: 10.0 }};
  }

  .footer {
    background-color: rgb(var(--background), var(--section-overlay-opacity));
  }   

  .footer__inner {
    padding: {{ section.settings.padding_desktop }};
  }

  .footer__item {
    height: 100%;
  }

  #shopify-section-{{ section.id }} .footer__item-title:after {
    content: '';
    display: none;
    width: 70px;
    height: 3px;
    background-color: var(--primary);
    margin-top: 15px;
  }

  .col-1 {
    width: 0px;
  }

  h2.footer__item-title.h6 {
    font-size: var(--base-font-size);
  }

  /* FOOTER TOP */
  .info-content_title {
    font-size: 14px;
    line-height: 20px;
    font-weight: 500;
  }
  .footer-top_icon svg {
    border: 1px solid var(--border-color);
    border-radius: 50%;
  }

  @media (min-width: 1200px) {
    #shopify-section-footer  .col-xl-2 {
        width: 15%;
    }
    {% if section.settings.show_newsletter %}
    .footer__item-list_newsletter {
      width: 42%;
    }
    .footer__item-list {
      width: 58%;
      padding-left: 130px;
    }
    {% endif %}
  }
  
  @media (max-width: 1500px) {
    .footer__inner {
      padding: {{ section.settings.padding_desktop_xl }};
    }
  }

  @media (max-width: 767px) {
    .footer__inner {
      padding: {{ section.settings.padding_mobile }}
    }
    .footer__item {
      padding: 0;
      border-right: none;
    }
    #shopify-section-{{ section.id }} .footer__item-title:after {
      margin-left: auto;
      margin-right: auto;
    }
    .footer-top_content.text-left {
      text-align: center !important;
    }
  }

  .newsletter-footer {
    padding: 20px;
    margin-bottom: 30px;
  }

  {%- for block in section.blocks -%}
    @media (min-width: 1200px) {
      #block-{{ block.id }} .footer__item {
        padding: {{ block.settings.padding_desktop }};
      }
    }
  {%- endfor -%}

  /* NEWSLETTER */
  .footer .input__label {
    color: {{ section.settings.color_input }};
  }
  .footer .input__field {
    color: {{ section.settings.color_input }};
    border: 1px solid {{ section.settings.border_color_input }};
  }
  .footer .button-newsletter {
    color: {{ section.settings.color_button }};
    background-color: {{ section.settings.bg_btn }};
  }
  .footer .button-newsletter:hover {
    color: {{ section.settings.color_button_hover }};
    background-color: {{ section.settings.bg_btn_hover }};
  }
</style>

<footer class="footer ">
  <div class="{%- if section.settings.fullwidth -%}container-fluid{%- else -%}container{%- endif -%}">
    <div class="footer__inner">

      <div class="{% if section.settings.show_newsletter %}footer__item-list{% endif %}">  

        {% if section.settings.show_newsletter %}
        <div class="footer__item-list_newsletter">
          <div class="footer__item newsletter-footer">
            <h2 class="footer__item-title heading {{ section.settings.heading_size }} mb-3">{{ section.settings.heading }}</h2>
            <p class="footer-description mb-30">{{ section.settings.description }}</p>
            <div {{ section.shopify_attributes }}>
              {% form 'customer', class: 'newsletter-form' %}
                <input type="hidden" name="form_type" value="customer">
                <input type="hidden" name="utf8" value="✓">
                <input type="hidden" name="contact[tags]" value="newsletter">
                <input type="hidden" name="contact[context]" value="newsletter-{{ section.id }}">

                <div class="wp-newsletter-input" style="background-color: {{ section.settings.bg_color }};">
                  <div class="input-row">
                    <div class="input input-mail">
                      <input style="background-color: {{ section.settings.bg_input }};" type="email" id="newsletter[{{ section.id }}][contact][email]" name="contact[email]" class="input__field" required="">
                      <label style="background-color: {{ section.settings.bg_input }};" for="newsletter[{{ section.id }}][contact][email]" class="input__label">{{ 'newsletter.label' | t }}</label>
                    </div>
                    <div class="input-icon d-none">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="#000000" width="800px" height="800px" viewBox="0 0 32 32">
                        <title/>
                        <g data-name="Layer 2" id="Layer_2">
                        <path d="M29,11H3a1,1,0,0,0-1,1V29a1,1,0,0,0,1,1H29a1,1,0,0,0,1-1V12A1,1,0,0,0,29,11Zm-2.79,2L16,20.23,5.79,13ZM4,28V14.18l11.42,8.09a1,1,0,0,0,1.16,0L28,14.18V28Z"/>
                        </g>
                      </svg>
                    </div>
                  </div>
                  <div class="input">
                    <button type="submit" is="loader-button" class="button button-newsletter">
                      <span class="ap-text-loaderbutton">
                        {{ section.settings.text_button }}
                      </span>
                      <span class="ap-loader-loaderbutton" hidden="">
                        <div class="spinner">
                          <svg focusable="false" width="24" height="24" class="icon icon--spinner" viewBox="25 25 50 50">
                            <circle cx="50" cy="50" r="20" fill="none" stroke="currentColor" stroke-width="5"></circle>
                          </svg>
                        </div>
                      </span>
                    </button>
                  </div>
                </div>

                {%- if form.errors -%}
                  <small class="newsletter-form__message form__message" id="Newsletter-error--{{ section.id }}">{% render 'icon-error' %}{{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}</small>
                {%- endif -%}
                {%- if form.posted_successfully? -%}
                  <div class="form__banner banner banner--success">
                    <span class="banner__ribbon">
                      <svg focusable="false" width="18" height="26" class="icon icon--form-success   " viewBox="0 0 18 26">
                        <circle fill="none" cx="9" cy="13" r="8" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></circle>
                        <path fill="none" d="M5 13l3 3 5-6" stroke="currentColor" stroke-width="1.5"></path>
                      </svg>
                    </span>
                    <p class="banner__content">{{ 'newsletter.success' | t }}</p>
                  </div>
                {%- endif -%}

              {% endform %}
            </div>
            <p class="footer-description_bottom mt-3">{{ section.settings.description_bottom }}</p>
          </div>
        </div>
        {% endif %}

        <div class="row">
          {%- for block in section.blocks -%}
            {%- case block.settings.width -%}
            {%- when '1%' -%}
            {%- assign block_width = 'col-1' -%}
            {%- when '16%' -%}
            {%- assign block_width = 'col-xl-2 col-md-6 col-12' -%}
            {%- when '20%' -%}
            {%- assign block_width = 'col-xl-20 col-md-6 col-12' -%}
            {%- when '23%' -%}
            {%- assign block_width = 'col-xl-23 col-md-6 col-12' -%}
            {%- when '25%' -%}
            {%- assign block_width = 'col-xl-3 col-md-6 col-12' -%}
            {%- when '33%' -%}
            {%- assign block_width = 'col-xl-4 col-md-6 col-12' -%}
            {%- when '40%' -%}
            {%- assign block_width = 'col-xl-36 col-md-6 col-12' -%}
            {%- when '50%' -%}
            {%- assign block_width = 'col-lg-6 col-md-12 col-12' -%}   
            {%- when '66%' -%}
            {%- assign block_width = 'col-xl-8 col-md-6 col-12' -%}
            {%- when '75%' -%}
            {%- assign block_width = 'col-xl-9 col-md-6 col-12' -%}
            {%- when '100%' -%}
            {%- assign block_width = 'col-12' -%}
            {%- endcase -%}
            <div id="block-{{ block.id }}" class="{{ block_width }} {{ block.settings.class }}">
  
              {%- case block.type -%}
  
                {%- when 'footer_text' -%}
                  <div class="footer__item text-{{ block.settings.text_align }} footer-item__{{ forloop.index }} {% if forloop.index == 1 %}is-first{% endif %}">
                    
                    {%- if block.settings.bottom_logo != blank -%}
                      <div class="logo-footer footer__item-title">
                        <img style="width: {{ block.settings.bottom_logo_width }}px" loading="lazy" sizes="{{ block.settings.bottom_logo_width }}px" class="footer__image" alt="" 
                          src="{{ block.settings.bottom_logo | image_url: width: block.settings.bottom_logo.width }}" 
                          data-srcset="{{ block.settings.bottom_logo | image_url: width: 100 }} 100w, {{ block.settings.bottom_logo | image_url: width: 200 }} 200w, {{ block.settings.bottom_logo | image_url: width: 300 }} 300w, {{ block.settings.bottom_logo | image_url: width: 400 }} 400w, {{ block.settings.bottom_logo | image_url: width: 500 }} 500w" 
                          width="{{ block.settings.bottom_logo.width }}" height="{{ block.settings.bottom_logo.height }}">
                      </div>
                    {% endif %}
  
                    {%- if block.settings.heading != blank -%}
                      <div class="footer__item-title heading {{ block.settings.heading_size }}">{{ block.settings.heading }}</div>
                    {% endif %}
  
                    {%- if block.settings.footer_description != blank -%}
                      <div class="footer__item-description mb-20">{{ block.settings.footer_description }}</div>
                    {% endif %}
                    
                    <div style="color: {{ section.settings.color_content_list }}" class="footer__item-content-list">
                      
                      {%- if block.settings.image != blank -%}
                      <div class="footer__item-content-img">
                        <img loading="lazy" sizes="{{ block.settings.image.width }}px" class="footer__image" alt=""
                          src="{{ block.settings.image | image_url: width: block.settings.image.width }}"
                          data-srcset="{{ block.settings.image | image_url: width: 100 }} 100w, {{ block.settings.image | image_url: width: 200 }} 200w, {{ block.settings.image | image_url: width: 300 }} 300w, {{ block.settings.image | image_url: width: 400 }} 400w, {{ block.settings.image | image_url: width: 500 }} 500w"
                          width="{{ block.settings.image.width }}" height="{{ block.settings.image.height }}">
                      </div>
                      {% endif %}
  
                      <div class="footer__item-content-text">
                        {%- if block.settings.content != blank -%}
                          <div class="footer__item-content">
                            {% if block.settings.content_icon != blank %}
                            <span>
                              {{ block.settings.content_icon }}
                            </span>
                            {% endif %}
                            {{ block.settings.content }}
                          </div>
                        {% endif %}
          
                        {%- if block.settings.content_2 != blank -%}
                          <div class="footer__item-content content-2">
                            {% if block.settings.content_icon_2 != blank %}
                            <span>
                              {{ block.settings.content_icon_2 }}
                            </span>
                            {% endif %}
                            {{ block.settings.content_2 }}
                          </div>
                        {% endif %}
          
                        {%- if block.settings.content_3 != blank -%}
                          <div class="footer__item-content content-3">
                            {% if block.settings.content_icon_3 != blank %}
                            <span>                  
                              {{ block.settings.content_icon_3 }}
                            </span>
                            {% endif %}
                            {{ block.settings.content_3 }}
                          </div>
                        {% endif %}
                      </div>
  
                    </div>
                    
                    {% liquid
                      assign _show_social = false
                      if block.settings.social_facebook != blank
                        assign _show_social = true
                      endif
                      if section.settings.social_tw != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_instagram != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_pinterest != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_youtube != blank
                        assign _show_social = true
                      endif
                      if block.settings.social_linkedin != blank
                        assign _show_social = true
                      endif
                    %}
                    {%- if _show_social == true -%}
                      {%- if block.settings.social_heading != blank -%}
                        <div class="footer__item-title heading {{ block.settings.heading_size }}">{{ block.settings.social_heading }}</div>
                      {%- endif -%}
                      <div class="footer__item-content">
                        <ul class="social-media social-media--no-radius list--unstyled" role="list">
                          {%- if block.settings.social_facebook != blank -%}
                            <li class="social-media__item social-media__item--facebook">
                              <a href="{{ block.settings.social_facebook }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Facebook"><svg focusable="false" width="9" height="17" class="icon icon--facebook   " viewBox="0 0 9 17">
