{{ 'slideshow.css' | asset_url | stylesheet_tag }}

{%- style -%}
  #shopify-section-{{ section.id }} {
    --progress-bar-color: {{ section.settings.progress_bar_color | color_extract: 'red' }}, {{ section.settings.progress_bar_color | color_extract: 'green' }}, {{ section.settings.progress_bar_color | color_extract: 'blue' }};
    --section-autoplay-duration: {{ section.settings.autoplay_duration }}s;
    --section-animation-play-state: paused;
    background-color: {{ section.settings.background_color }};

    position: relative;

    --slide-font-size: {{ section.settings.slide_font_size }}px;
    --slide-font-weight: {{ section.settings.slide_font_weight }};
    --slide-font-spacing: {{ section.settings.slide_font_spacing }}em;
    --slide-font-lineheight: {{ section.settings.slide_font_lineheight }};
    --slide-font-transform: {{ section.settings.slide_font_transform }};
  }
  {% for block in section.blocks %}
    #block-{{ block.id }} {
      --heading-color: {{ block.settings.heading_color | color_extract: 'red' }}, {{ block.settings.heading_color | color_extract: 'green' }}, {{ block.settings.heading_color | color_extract: 'blue' }};
      --color-body: {{ block.settings.text_color | color_extract: 'red' }}, {{ block.settings.text_color | color_extract: 'green' }}, {{ block.settings.text_color | color_extract: 'blue' }};
      --primary-button-background: {{ block.settings.button_background | color_extract: 'red' }}, {{ block.settings.button_background | color_extract: 'green' }}, {{ block.settings.button_background | color_extract: 'blue' }};
      --primary-button-text-color: {{ block.settings.button_text_color | color_extract: 'red' }}, {{ block.settings.button_text_color | color_extract: 'green' }}, {{ block.settings.button_text_color | color_extract: 'blue' }};
      --primary-button-text-hover: {{ block.settings.button_text_color_hover | color_extract: 'red' }}, {{ block.settings.button_text_color_hover | color_extract: 'green' }}, {{ block.settings.button_text_color_hover | color_extract: 'blue' }};
      --primary-button-bg-hover: {{ block.settings.button_bg_hover | color_extract: 'red' }}, {{ block.settings.button_bg_hover | color_extract: 'green' }}, {{ block.settings.button_bg_hover | color_extract: 'blue' }};
      --button-overlay-opacity: {{ block.settings.button_opacity | divided_by: 10.0 }};

      --section-blocks-alignment: center;
      --section-blocks-overlay-color: {{ block.settings.blocks_overlay_color | color_extract: 'red' }}, {{ block.settings.blocks_overlay_color | color_extract: 'green' }}, {{ block.settings.blocks_overlay_color | color_extract: 'blue' }};
      --section-blocks-overlay-opacity: {{ block.settings.overlay_opacity | divided_by: 10.0 }};
    }

    #block-{{ block.id }} .slideshow__image {
      -o-object-position: {{ block.settings.position_image_mb }};
      object-position: {{ block.settings.position_image_mb }};
    }
  {% endfor %}

  #shopify-section-{{ section.id }} .slide__heading {
    font-weight: var(--slide-font-weight);
    font-size: var(--slide-font-size);
    line-height: var(--slide-font-lineheight);
    letter-spacing:var(--slide-font-spacing);
    text-transform: var(--slide-font-transform);
  }

  {% if section.settings.show_bg_content %}
  #shopify-section-{{ section.id }} .content-box {
    padding: 70px;
    background-color: var(--primary);
    border-radius: 10px;
    max-width: 490px;
  }
  {% endif %}

  #shopify-section-{{ section.id }} .button-group .button {
    margin: 0;
  }

  /* ARROW */
  #shopify-section-{{ section.id }} .ap-listslideshow__arrow {
    background-color: {{ section.settings.bg_btn_arrow }};
    color: {{ section.settings.color_btn_arrow }};
    border: none;
    width: 60px;
    height: 60px;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    bottom: unset;
    z-index: 1;
    transition: all .4s;
    opacity: 0;
    visibility: hidden;
    border-radius: 20px;
  }
  #shopify-section-{{ section.id }} .slideshow:hover .ap-listslideshow__arrow {
    opacity: 1;
    visibility: visible;
  }
  #shopify-section-{{ section.id }} .slideshow:hover .ap-listslideshow__arrow:hover {
    background-color: var(--primary-2);
    color: {{ section.settings.bg_btn_arrow }};
  }
  #shopify-section-{{ section.id }} .ap-listslideshow__arrow i.fa {
    font-size: 16px;
  }
  #shopify-section-{{ section.id }} .sl-btn-prev {
    left: 90px;
    right: unset;
  }
  #shopify-section-{{ section.id }} .sl-btn-next {
    right: 90px;
    left: unset;
  }
  #shopify-section-{{ section.id }} .slideshow:hover .sl-btn-prev {
    left: 50px;
  }
  #shopify-section-{{ section.id }} .slideshow:hover .sl-btn-next {
    right: 50px;
  }
  #shopify-section-{{ section.id }} .ap-listslideshow__ap-button-nextprev {
    display: block;
  }
  /* END */

  /* DOT */
  #shopify-section-{{ section.id }} .slideshow__progress-bar:before, .slideshow__progress-bar:after {
    height: 8px;
  }
  .slideshow__progress-bar[ap-currentaria="true"]:before {
    background: var(--primary);
    width: 24px;
  }
  #shopify-section-{{ section.id }} .slideshow__nav {

  }
  /* end */

  @media (max-width: 991px) {
    {% if section.settings.show_bg_content %}
      #shopify-section-{{ section.id }} .content-box {
      padding: 30px;
      max-width: 400px;
    }
    {% endif %}
    #shopify-section-{{ section.id }} .slide__heading {
      font-size: 60px;
    }
  }

@media only screen and (max-width:749px) {
  .mobile--slideshow-250{
    --slideshow-height: 250px !important;
  }
  .mobile--slideshow-300{
    --slideshow-height: 300px !important;
  }
  .mobile--slideshow-400{
    --slideshow-height: 400px !important;
  }
  .mobile--slideshow-500{
    --slideshow-height: 500px !important;
  }
  .mobile--slideshow-full{
    --slideshow-height: 100vh !important;
  }

  #shopify-section-{{ section.id }} .slide__heading {
    font-size: 38px;
  }
}

@media only screen and (min-width:750px) {
  .slideshow--desktop-900{
    --slideshow-height: 900px;
  }
  .slideshow--desktop-830{
    --slideshow-height: 830px;
  }
  .slideshow--desktop-750{
    --slideshow-height: 750px;
  }
  .slideshow--desktop-670 {
    --slideshow-height: 670px;
  }
  .slideshow--desktop-550{
    --slideshow-height: 100px;
  }
  .slideshow--desktop-full{
    --slideshow-height: 100vh;
  }
}

.slideshow__slide {
  height: var(--slideshow-height)
}
{%- endstyle -%}

{% render 'section-style' %}

<section class="section section--flush {{ section.settings.class }}">
  <div class="{{ section.settings.setwidthout }}">
    <div class="slide-row">
      <ap-slideshow reveal-on-scroll="" {% if section.settings.auto_play %}auto-play=""{% endif %} transition-type="sweep" class="slideshow {{ section.settings.slideshow_height }} {{ section.settings.slideshow_height_mobile }}  ap-object-loaded" style="">
        <div class="slideshow__slide-list">
  
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when 'slide' -%}
                <ap-slideshowitem ap-revealvisibility="" id="block-{{ block.id }}" class="slideshow__slide {% if block.settings.image_2 %}slideshow__slide--split{% endif %} slideshow__slide--sweep" {% if forloop.first %}style="visibility: visible;"{% else %}style="visibility: hidden;"{% endif %} {% if forloop.index > 1 %}hidden=""{% endif %}>
                  <div class="slideshow__slide-inner">
                    {% if block.settings.image_1 %}
                      <div class="slideshow__image-wrapper slideshow__image-wrapper--first">
                        <img src="{{ block.settings.image_1 | image_url: width: 3000 }}" 
                         data-srcset="{{ block.settings.image_1 | image_url: width: 400 }} 400w, {{ block.settings.image_1 | image_url: width: 500 }} 500w, {{ block.settings.image_1 | image_url: width: 600 }} 600w, {{ block.settings.image_1 | image_url: width: 700 }} 700w, {{ block.settings.image_1 | image_url: width: 800 }} 800w, {{ block.settings.image_1 | image_url: width: 900 }} 900w, {{ block.settings.image_1 | image_url: width: 1000 }} 1000w, {{ block.settings.image_1 | image_url: width: 1200 }} 1200w, {{ block.settings.image_1 | image_url: width: 3000 }} 1400w" 
                          loading="eager" draggable="false" class="slideshow__image" sizes="100vw" width="{{ block.settings.image_1.width }}" height="{{ block.settings.image_1.height }}" alt="">
                      </div>
                    {% endif %}   
                    {% if block.settings.image_2 %}
                      <div class="slideshow__image-wrapper slideshow__image-wrapper--secondary">
                        <img src="{{ block.settings.image_2 | image_url: width: 3000 }}" 
                         data-srcset="{{ block.settings.image_2 | image_url: width: 400 }} 400w, {{ block.settings.image_2 | image_url: width: 500 }} 500w, {{ block.settings.image_2 | image_url: width: 600 }} 600w, {{ block.settings.image_2 | image_url: width: 700 }} 700w, {{ block.settings.image_2 | image_url: width: 800 }} 800w, {{ block.settings.image_2 | image_url: width: 900 }} 900w, {{ block.settings.image_2 | image_url: width: 1000 }} 1000w, {{ block.settings.image_2 | image_url: width: 1200 }} 1200w, {{ block.settings.image_2 | image_url: width: 3000 }} 1400w" 
                          loading="eager" draggable="false" class="slideshow__image" sizes="100vw" width="{{ block.settings.image_2.width }}" height="{{ block.settings.image_2.height }}" alt="">
                      </div>
                    {% endif %}
                    {% if block.settings.image_mobile %}
                      <div class="slideshow__image-wrapper hd-tablet-s">
                        <img src="{{ block.settings.image_mobile | image_url: width: 3000 }}" 
                         data-srcset="{{ block.settings.image_mobile | image_url: width: 400 }} 400w, {{ block.settings.image_mobile | image_url: width: 500 }} 500w, {{ block.settings.image_mobile | image_url: width: 600 }} 600w, {{ block.settings.image_mobile | image_url: width: 700 }} 700w, {{ block.settings.image_mobile | image_url: width: 800 }} 800w, {{ block.settings.image_mobile | image_url: width: 900 }} 900w, {{ block.settings.image_mobile | image_url: width: 1000 }} 1000w, {{ block.settings.image_mobile | image_url: width: 1200 }} 1200w, {{ block.settings.image_mobile | image_url: width: 3000 }} 1400w" 
                          loading="eager" draggable="false" class="slideshow__image" sizes="100vw" width="{{ block.settings.image_mobile.width }}" height="{{ block.settings.image_mobile.height }}" alt="">
                      </div>
                    {% endif %}
  
                    <div class="{{ section.settings.setwidthout_content }}">
                      <ap-splitlines class="slideshow__text-wrapper slideshow__text-wrapper--middle vertical-breather content-box--{{ block.settings.box_align }}">
                        <div reveal="" class="content-box content-box--{{ block.settings.box_size }} content-box--text-{{ block.settings.text_align }} text-container">
                          {% if block.settings.subheading != blank %}
                            <h3 style="color: {{ block.settings.color_subheading }}" class="sub-heading mb-10">
                                {% assign subheading = block.settings.subheading | split: "|" %}
                                {% for item in subheading %}
                                  <span style="display: block;">{{ item | strip }}</span>
                                {% endfor %}
                            </h3>
                          {% endif %}
                          {% if block.settings.heading != blank %}
                            <h2 class="heading slide__heading my-0 mb-14">
                                {% assign heading = block.settings.heading | split: "|" %}
                                {% for item in heading %}
                                  <span style="display: block;">{{ item | strip }}</span>
                                {% endfor %}
                            </h2>
                          {% endif %}
                          {% if block.settings.description != blank  %}
                          <div style="color: {{ block.settings.color_des }}" class="description mb-md-3 pb-1">
                              {% assign description = block.settings.description | split: "|" %}
                              {% for item in description %}
                                <span style="display: block;">{{ item | strip }}</span>
                              {% endfor %}
                          </div>
                          {% endif %}
                          <div style="{% if block.settings.slide_morecontent != blank %}gap: 20px;{% endif %}" class="button-group {% if block.settings.slide_morecontent != blank %}d-flex flex-wrap{% endif %}">
                            <style>
                              #block-{{ block.id }} .btn-outline, 
                              #block-{{ block.id }} .btn-line ,  
                              #block-{{ block.id }} .btn-base {
                                color: {{ block.settings.color_btn }};
                                background-color: {{ block.settings.bg_btn }};
                                border-color: {{ block.settings.bg_btn }};
                              }
                              #block-{{ block.id }} .btn-outline, 
                              #block-{{ block.id }} .btn-line {
                                background-color: transparent;
                              }
                              #block-{{ block.id }} .btn-outline {
                                border-color: {{ block.settings.bg_btn }};
                              }
                              #block-{{ block.id }} .btn-outline:hover, 
                              #block-{{ block.id }} .btn-line:hover ,
                              #block-{{ block.id }} .btn-base:hover {
                                color: {{ block.settings.color_btn_hover }};
                                background-color: {{ block.settings.bg_btn_hover }};
                                border-color: {{ block.settings.bg_btn_hover }};
                              }
                              #block-{{ block.id }} .btn-line:hover {
                                background-color: transparent;
                              }
                            </style>
                            {% if block.settings.button1_label == blank or block.settings.button2_label == blank %}
                              <div class="button-wrapper">
                            {% else %}
                              <div class="button-group__wrapper">
                            {% endif %}
                              {%- if block.settings.button1_label != blank -%}
                                <a href="{{ block.settings.link1 }}" class="button {{ block.settings.style_btn }}">
                                  <span>
                                    {{- block.settings.button1_label -}}
                                  </span>
                                </a>
                              {%- endif -%}
                              {% if block.settings.button2_label != blank %}
                                <a href="{{ block.settings.link2 }}" class="button {{ block.settings.style_btn_2 }}">
                                  <span>
                                    {{- block.settings.button2_label -}}
                                  </span>
                                </a>
                              {% endif %}
                              </div>
                              
                              {% if block.settings.slide_morecontent != blank %}
                              <span class="slideshow-morecontent">
                                {{ block.settings.slide_morecontent }}
                              </span>
                              {% endif %}

                            </div>
                        </div>
                      </ap-splitlines>
                      </div>
                      
                  </div>
                </ap-slideshowitem>
  
            {%- endcase -%}
          {%- endfor -%}
        </div>
        {% if section.settings.show_dot %}
        <ap-pagedots animation-timer="" class="slideshow__nav container">
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when 'slide' -%}
                <button class="slideshow__progress-bar" ap-controlsaria="block-{{ block.id }}" ap-currentaria="{% if forloop.index == 1 %}true{% else %}false{% endif %}">
                </button>
            {%- endcase -%}
          {%- endfor -%}
        </ap-pagedots>
        {% endif %}
        {% if section.settings.show_btn %}
          <ap-button-nextprev class="ap-listslideshow__ap-button-nextprev ap-button-nextprev ap-button-nextprev--row">
              <button class="ap-listslideshow__arrow ap-nextap-buttonprev ap-nextap-buttonprev--prev sl-btn-prev">
                  <span class="visually-hidden">Previous</span>
                  <i class="fa fa-light fa-chevron-left"></i>
              </button>
    
              <button class="ap-listslideshow__arrow ap-nextap-buttonprev ap-nextap-buttonprev--next sl-btn-next">
                  <span class="visually-hidden">Next</span>
                  <i class="fa fa-light fa-chevron-right"></i>
              </button>
          </ap-button-nextprev>
        {% endif %}
        <div class="slideshow-social">
          {% liquid
            assign _show_social = false
            if section.settings.social_facebook != blank
              assign _show_social = true
            endif
            if section.settings.social_tw != blank
              assign _show_social = true
            endif
            if section.settings.social_pinterest != blank
              assign _show_social = true
            endif
            if section.settings.social_linkedin != blank
              assign _show_social = true
            endif      
          %}
          {%- if _show_social == true -%}
            <div class="slideshow__item-content">
              <ul class="social-media social-media--no-radius list--unstyled" role="list">
                {%- if section.settings.social_facebook != blank -%}
                  <li class="social-media__item social-media__item--facebook">
                    <a href="{{ section.settings.social_facebook }}" target="_blank" rel="noopener" class="social-media__link" aria-label="Follow us on Facebook">
                    <svg focusable="false" width="9" height="17" class="icon icon--facebook   " viewBox="0 0 9 17">
